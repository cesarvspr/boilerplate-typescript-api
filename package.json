{
  "name": "template-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "postbuild": "cpy '**/*' '!**/*.ts' ../dist --cwd=src --no-overwrite --parents",
    "start:dev": "ts-node-dev --transpile-only --inspect=0.0.0.0:9229 -r dotenv/config -r tsconfig-paths/register --files src/server.ts",
    "start": "node -r source-map-support/register dist/server",
    "test": "./scripts/run-tests.sh",
    "test:mocha": "DOTENV_CONFIG_PATH=.env.test mocha -r ts-node/register -r tsconfig-paths/register -r dotenv/config -p tsconfig.json test/**/*.test.ts",
    "mocha": "DOTENV_CONFIG_PATH=.env.local mocha -r ts-node/register -r tsconfig-paths/register -r dotenv/config -p tsconfig.json test/**/*.test.ts",
    "db:clear": "./scripts/clear-db.sh",
    "migration": "npx sequelize-cli db:migrate",
    "migration-undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "prepare": "is-ci || husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesarvspr/boilerplate-typescript-api.git"
  },
  "author": "",
  "license": "ISC",
